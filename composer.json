{
    "name": "callcocam/tall-acl",
    "type": "package",
    "description": "The packages acl table.",
    "keywords": ["framework", "laravel", "livewire", "alpinejs", "tailwindcss"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/jetstream": "^2.5",
        "livewire/livewire": "^2.5",
        "wireui/wireui": "^1.5",
        "callcocam/tall-schema": "dev-main",
        "callcocam/tall-sluggable": "dev-main",
        "callcocam/tall-status": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Tall\\Acl\\": "src"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Tall\\Acl\\AclServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php artisan  jetstream:install livewire"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
